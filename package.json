{
  "name": "blog-app-web-site",
  "version": "0.1.0",
  "description": "blog web site",
  "main": "src/index.js",
  "repository": "https://github.com/damiangamracy/blog-app-web-site.git",
  "author": "Damian Gamracy <damiangamracy@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "start": "webpack-dev-server --open --mode development",
    "format": "run-p format:*",
    "format:es": "prettier src/**/*.js --write",
    "format:style": "prettier src/**/*.scss --parser scss --write",
    "lint": "run-p lint:*",
    "lint:es": "eslint src/**/*.js --fix",
    "lint:style": "stylelint src/**/*.scss --syntax scss --fix",
    "fix": "run-p fix:*",
    "fix:es": "run-s format:es lint:es",
    "fix:style": "run-s format:style lint:style",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "recompose": "^0.30.0",
    "redux": "^4.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.1",
    "eslint": "^5.9.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.11.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.2.0",
    "lint-staged": "^8.1.0",
    "node-sass": "^4.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "webpack": "^4.22.0",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.9"
  }
}
